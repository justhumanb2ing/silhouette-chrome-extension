{
  "name": "Poketto Chrome Extension",
  "displayName": "__MSG_extensionName__",
  "version": "0.0.1",
  "description": "__MSG_extensionDescription__",
  "author": "KINMONGSANG",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@clerk/chrome-extension": "^2.8.13",
    "@plasmohq/storage": "^1.15.0",
    "@supabase/supabase-js": "^2.89.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "^7.11.0",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "default_locale": "ko",
    "key": "$CRX_PUBLIC_KEY",
    "permissions": [
      "cookies",
      "scripting",
      "storage",
      "tabs"
    ],
    "commands": {
      "save-current-page": {
        "suggested_key": {
          "default": "Ctrl+Shift+S",
          "mac": "Command+Shift+S"
        },
        "description": "Save current page URL"
      }
    },
    "host_permissions": [
      "$PLASMO_PUBLIC_CLERK_SYNC_HOST/*",
      "http://localhost/*",
      "$CLERK_FRONTEND_API/*",
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ]
  }
}
